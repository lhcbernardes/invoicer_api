{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "build": {
      "env": {
        "NPM_TOKEN": "@npm_token",
        "API_URL": "@api_url",
        "IS_NOW": "true"
      }
    }
}